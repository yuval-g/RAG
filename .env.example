# Example environment variables for the RAG Engine
# Copy this file to .env and fill in your actual values

# LangChain Tracing (Optional)
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your-langchain-api-key

# API Server Configuration
RAG_API_HOST=0.0.0.0
RAG_API_PORT=8000
RAG_API_RELOAD=false

# General Application Configuration
RAG_ENVIRONMENT=development
RAG_CONFIG_PATH=
RAG_LOG_LEVEL=INFO
RAG_ENGINE_ENV=development
RAG_ENGINE_LOG_FILE=rag_engine.log
RAG_ENGINE_STRUCTURED_LOGS=false

# LLM Provider API Keys
GOOGLE_API_KEY=your-google-api-key-here
OPENAI_API_KEY=your-openai-api-key-here

# Vector Store API Keys
PINECONE_API_KEY=your-pinecone-api-key-here
WEAVIATE_URL=your-weaviate-url-here
WEAVIATE_API_KEY=your-weaviate-api-key-here

# Optional: Model configuration
DEFAULT_LLM_MODEL=gemini-2.0-flash-lite
DEFAULT_TEMPERATURE=0.1

# Optional: Set user agent for requests
USER_AGENT=rag-engine/1.0
